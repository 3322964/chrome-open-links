{
  "name": "Open All Links",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Opens all links within the currently selected area in new tabs.",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["build/links.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/content.js"]
    }
  ]
}
